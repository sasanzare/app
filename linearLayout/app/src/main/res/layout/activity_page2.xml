      <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        android:width="48dp"
        android:height="48dp"
        android:background="#00BCD4"
        android:gravity="center"
        android:text="1"
        android:textColor="#FFFFFF" />

